{
  "name": "app",
  "version": "1.0.0",
  "description": "A demo app built using ES2015, babel, webpack",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d & copy \"src/index.html\" \"dist/index.html\" /Y & webpack-dev-server --content-base src/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p & copy \"src/index.html\" \"dist/index.html\" /Y"
  },
  "keywords": [
    "es2015",
    "babel",
    "webpack"
  ],
  "author": "Piyush Abbi",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.1",
    "dialog-polyfill": "^0.4.7",
    "file-loader": "^0.9.0",
    "firebase": "^3.6.9",
    "history": "^1.17.0",
    "imports-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "imports-loader": "^0.7.1",
    "jquery": "^3.1.1",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3"
  }
}
